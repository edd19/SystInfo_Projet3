set title 'Benchmark of writev/lseek+write'
set xlabel 'size of buffer [B]'
set ylabel 'time [ns]'
set key right top
set logscale x
plot 'writev.csv' using 1:2 title 'writev(1mb file)',\
  'lseek.csv' using 1:2 title 'lseek+write(1mb file)',\
  'writev2.csv' using 1:2 title 'writev(4mb file)',\
  'lseek2.csv' using 1:2 title 'lseek+write(4mb file)'
