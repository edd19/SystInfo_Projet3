set title 'Benchmark of writev/lseek+write'
set xlabel 'size of buffer [B]'
set ylabel 'time [ns]'
set key right top
set logscale x
plot 'writev.csv' using 1:2 title 'writev(32mb file)',\
  'lseek.csv' using 1:2 title 'lseek+write(32mb file)',\
  'writev2.csv' using 1:2 title 'writev(64mb file)',\
  'lseek2.csv' using 1:2 title 'lseek+write(64mb file)'
